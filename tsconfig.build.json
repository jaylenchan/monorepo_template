{
    "compilerOptions": {
        "strict": true,
        "useUnknownInCatchVariables": true,
        "allowJs": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "newLine": "lf",
        "noEmitOnError": true,
        "declaration": true,
        "noEmit": false,
    },
    "exclude": [
        "**/node_modules",
        "**/.*/",
        "dist"
    ]
}